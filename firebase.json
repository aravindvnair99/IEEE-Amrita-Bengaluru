{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source" : "**/*.@(jpg|jpeg|gif|png|css|js)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=3600"
      } ]
      }, {
      "source" : "https://www.google-analytics.com/analytics.js",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=7200"
      } ]
    } ]
  }
}
